%div(style="width:100%;height:100px;margin-left:30%")
  =form_for :add_a_new_xlsx ,url:arrange_add_a_new_xlsx_path do |f|
    =f.file_field :excel_sheet, :id=>'gift_pack_excel_input'
    %br
    =f.submit '保存', :id=>'submit_excel',:style=>"margin-top:20px"
    %br
    -if flash[:error]
      =flash[:error]
%div(style="width:100%;height:50px;margin-left:30%;margin-top:20px")
  %button(style="margin-right:20px" )
    =link_to "处理数据" ,arrange_delete_path
  %button(style="margin-right:20px" )
    = link_to '下载  Excel1', arrange_daochu_path(:format => :xls)
  %button(style="margin-right:20px" )
    = link_to '下载  Excel2', arrange_daochu_excle2_path(:format => :xls)
  %br
  %button(style="margin-right:20px;margin-top:20px" )
    = link_to '查看数据', arrange_show_excle_path


